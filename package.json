  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nodemon",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "pm2 start ecosystem.config.js --env production",
